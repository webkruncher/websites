
<Page name="Home">
	<t>Home</t>
	<Content role="" >
		<p> 
			Authroized personel only beyond this point
		</p>
	</Content>
	<Content role="admin,user" >
		<p> 
			Login:
		</p>
		<p>
			<form id="LoginForm" >
				<table>
					<tr><td>User name:</td><td><INPUT name="UserName" /> </td></tr>
